{
  "name": "data-processing",
  "version": "1.0.0",
  "main": "dist/index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "prisma:generate": "npm run prisma:generate:mqtt && npm run prisma:generate:loggers",
    "prisma:push": "npm run prisma:push:loggers",
    "prisma:migrate": "cross-env PRISMA_SCHEMA=prisma/data-loggers/schema.prisma PRISMA_DATABASE_URL=$DATA_LOGGERS_DATABASE_URL npx prisma migrate dev --schema=prisma/data-loggers/schema.prisma",
    "prisma:deploy": "cross-env PRISMA_SCHEMA=prisma/data-loggers/schema.prisma PRISMA_DATABASE_URL=$DATA_LOGGERS_DATABASE_URL npx prisma migrate deploy --schema=prisma/data-loggers/schema.prisma",
    "prisma:studio": "cross-env PRISMA_SCHEMA=prisma/data-loggers/schema.prisma PRISMA_DATABASE_URL=$DATA_LOGGERS_DATABASE_URL npx prisma studio --schema=prisma/data-loggers/schema.prisma"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "Data Processing Service - Process MQTT messages from staging to target databases",
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "cross-env": "^7.0.3",
    "prisma": "^7.1.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "dotenv": "^17.2.3",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "zod": "^4.1.13"
  }
}
